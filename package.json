{
  "name": "fantasticon-playground",
  "version": "1.0.0",
  "description": "Custom icon font CDN using Fantasticon and GitHub Pages",
  "scripts": {
    "build": "bash process_fonts.sh && node ./generate_index.js",
    "dev": "fantasticon src/font/svg/ -o dist/assets/font/ --watch",
    "clean": "rm -rf dist",
    "process-fonts": "bash process_fonts.sh"
  },
  "devDependencies": {
    "fantasticon": "^3.0.0"
  },
  "dependencies": {
    "sharp": "^0.34.2",
    "svgo": "^3.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sunwood-ai-labs/fantasticon-playground.git"
  },
  "homepage": "https://sunwood-ai-labs.github.io/fantasticon-playground/",
  "keywords": [
    "icon-font",
    "cdn",
    "github-pages",
    "fantasticon"
  ]
}
